function script()local a=4093362;local function b(c)return c>12 and c-12 or c end;game:GetService("RunService").RenderStepped:Connect(function()local d=os.date("*t")currenttime=string.format("%s:%s:%s %s",d.hour>=10 and b(d.hour)or"0"..tostring(d.hour),d.min>=10 and d.min or"0"..tostring(d.min),d.sec>=10 and d.sec or"0"..tostring(d.sec),d.hour>12 and"PM"or"AM")end)task.spawn(function()while autoFarm do wait(math.random(5,220))for e,e in pairs(game:GetService("Workspace").Tycoons:GetChildren())do if e.Owner.Value==tostring(game.Players.LocalPlayer)then for e,e in pairs(e.PlacedContent:GetChildren())do if e.Name=="Bank Vault"then fireclickdetector(e.Model.Detector.ClickDetector)else if e.Name=="Vending Machine"then fireclickdetector(e.Model.Click.ClickDetector)else if e.Name=="Safe"then fireclickdetector(e.Model.Detector.ClickDetector)end end end end end end;game:GetService("ReplicatedStorage").Events.SessionRewards:FireServer("Reward1")game:GetService("ReplicatedStorage").Events.SessionRewards:FireServer("Reward2")game:GetService("ReplicatedStorage").Events.SessionRewards:FireServer("Reward3")game:GetService("ReplicatedStorage").Events.SessionRewards:FireServer("Reward4")game:GetService("ReplicatedStorage").Events.SessionRewards:FireServer("Reward5")rconsoleprint(currenttime.." - Collected Vaults,Safes and Vending Machines!\n")end end)task.spawn(function()while autoCrate do wait(math.random(1,20))local f=game.Workspace:FindFirstChild("Godly Crate")or game.Workspace:FindFirstChild("Legendary Crate")or game.Workspace:FindFirstChild("Rainbow Crate")or game.Workspace:FindFirstChild("Dark Matter Crate")or game.Workspace:FindFirstChild("Blazing Crate")or game.Workspace:FindFirstChild("Base Crate")if f then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=f.Union.CFrame;rconsoleprint(currenttime.." - Collected Crate\n")end end end)task.spawn(function()if antiAFK then for g,h in pairs(getconnections(game.Players.LocalPlayer.Idled))do h:Disable()end end end)task.spawn(function()if game.Players.LocalPlayer.UserId=="21432915"then game.Players.LocalPlayer:Kick("Cmon dave")end end)task.spawn(function()game:GetService("Players").PlayerAdded:Connect(function(i)if i:IsInGroup(a)and i:GetRankInGroup(a)>=101 then game.Players.LocalPlayer:Kick("Staff joined the game.")rconsoleerr(currenttime.." - Kicked due to a staff joing your game!\n")end end)end)task.spawn(function()rconsolename("Base Raiders Script Console")rconsoleclear()rconsoleprint("@@RED@@")rconsoleprint("Thanks for using my script, "..game.Players.LocalPlayer.Name.."!\n")rconsoleprint("All credits to Unnamed#0152\n")rconsoleprint("Make sure to join the discord for more releases!\ndiscord.gg/YJJuKp9Eag\n")rconsoleprint("@@WHITE@@")end)task.spawn(function()if autoFarm then rconsoleprint("Auto-Farm Active.\n")else rconsoleprint("Auto-Farm Disabled.\n")end end)task.spawn(function()if autoCrate then rconsoleprint("Auto-Crate Active.\n")else rconsoleprint("Auto-Crate Disabled.\n")end end)task.spawn(function()if antiAFK then rconsoleprint("Anti-AFK Active.\n")else rconsoleprint("Anti-AFK Disabled.\n")end end)end;if syn then script()else game.Players.LocalPlayer:Kick("Synapse X Is Only Supported!")end
